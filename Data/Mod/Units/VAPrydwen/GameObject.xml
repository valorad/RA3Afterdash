					StateName="STATE_Selected">
					<Animation
						AnimationName="AULegionnaireInfantry_AIDA"
						AnimationMode="ONCE"
						AnimationPriority="100"
						AnimationBlendTime="15"/>
						<Animation
						AnimationName="AULegionnaireInfantry_AIFA"
